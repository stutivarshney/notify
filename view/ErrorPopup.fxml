<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<StackPane prefHeight="133.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ErrorHandler">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="127.0" prefWidth="350.0">
         <children>
            <Label text="Error!">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <Label prefHeight="52.0" prefWidth="294.0" text="Application running! Check your system tray!" wrapText="true" />
            <Button fx:id="errClose" alignment="BOTTOM_RIGHT" mnemonicParsing="false" onAction="#onClose" prefHeight="18.0" prefWidth="56.0" text="Close" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</StackPane>
